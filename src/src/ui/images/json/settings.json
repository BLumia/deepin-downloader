{
    "groups":[{
            "key":"Basic",
            "name":"基本设置",
            "groups":[{
                    "key":"Start",
                    "name":"启动",
                    "options":[{
                            "key":"PowerOn",
                            "name":"开机启动",
                            "type":"switchbutton",
                            "default":true
                        },
                        {
                            "key":"AutoStartUnfinishedTask",
                            "name":"启动后自动开始未完成的任务",
                            "type":"switchbutton",
                            "default":false
                        }

                    ]
                },
                {
                    "key":"DownloadDirectory",
                    "name":"下载目录",
                    "options":[
                        {
                            "key": "downloadDirectoryFileChooser",
                            "name": "",
                            "type": "filechooseredit",
                            "default":"custom;"
                        }
                    ]
                },
                {
                    "key":"OnekeyDownload",
                    "name":"一键下载",
                    "options":[
                        {
                            "key":"onekeydownload",
                            "name":"一键下载",
                            "type":"switchbutton",
                            "default":false
                        }
                    ]
                },
                {
                    "key":"CloseMainWindow",
                    "name":"关闭主界面",
                    "options":[
                        {
                            "key": "disckcacheNum",
                            "type": "radiogroup",
                            "items": [
                              "最小化到托盘",
                              "退出下载器"
                            ],
                            "default": 1
                        }
                    ]
                }
            ]
        },
        {
            "key":"DownloadTaskManagement",
            "name":"任务管理",
            "groups":[{
                    "key":"downloadtaskmanagement",
                    "name":"",
                    "options":[{
                            "key":"MaxDownloadTask",
                            "name":"同时下载最大任务数",
                            "type":"combobox",
                            "default":"5"
                        },
                        {
                            "key":"AutoOpen",
                            "name":"下载完成后自动打开",
                            "type":"switchbutton",
                            "default":true
                        },
                        {
                            "key":"AutoDelete",
                            "name":"自动删除“文件不存在”的任务",
                            "type":"switchbutton",
                            "default":false
                        }
                    ]
                }
            ]
        },
        {
            "key":"DownloadSettings",
            "name":"下载设置",
            "groups":[{
                    "key":"downloadsettings",
                    "name":"",
                    "options":[{
                            "key":"downloadspeedlimit",
                            "name":"",
                            "type":"downloadspeedlimitsetting",
                            "default":"speedlimit;10240;32;08:00:00;17:00:00"
                        }
                    ]
                }
            ]
        },
        {
            "key":"Monitoring",
            "name":"接管设置",
            "groups":[{
                    "key":"MonitoringObject",
                    "name":"接管对象",
                    "options":[{
                            "key":"ClipBoard",
                            "name":"剪切板",
                            "type":"switchbutton",
                            "default":true
                        }
                    ]
                },
                {
                    "key":"MonitoringDownloadType",
                    "name":"接管下载类型",
                    "options":[{
                            "key":"HttpDownload",
                            "name":"",
                            "type":"httpdownload",
                            "default":true
                        },
                        {
                            "key":"BTDownload",
                            "name":"",
                            "type":"btdownload",
                            "default":true
                        },
                        {
                            "key":"MagneticDownload",
                            "name":"",
                            "type":"magneticdownload",
                            "default":false
                        }
                    ]
                },
                {
                    "key":"BTRelation",
                    "name":"BT关联",
                    "options":[{
                            "key":"OpenDownloadPanel",
                            "name":"下载种子文件后自动打开新建窗口",
                            "type":"switchbutton",
                            "default":false
                        },
                        {
                            "key":"AssociateBTFileAtStartup",
                            "name":"启动时关联BT种子文件(.torrent文件)",
                            "type":"switchbutton",
                            "default":true
                        }
                    ]
                }
            ]
        },
        {
            "key":"Notifications",
            "name":"通知提醒",
            "groups": [{
                    "key": "remind",
                    "name": "",
                    "options": [{
                            "key": "downloadInfoNotify",
                            "name": "下载完成/失败时，系统通知提醒",
                            "type": "switchbutton",
                            "default": true
                        },
                        {
                            "key": "afterDownloadPlayTone",
                            "name": "下载完成后，播放提示音",
                            "type": "switchbutton",
                            "default": true
                        }
                ]
              }
            ]
        },
        {
            "key":"AdvancedSetting",
            "name":"高级设置",
            "groups": [{
                    "key": "ShortcutKeySetting",
                    "name": "快捷设置",
                    "options": [{
                            "key": "NewTaskShowMainwindow",
                            "name": "新建任务时显示主界面",
                            "type": "switchbutton",
                            "default": true
                        },
                        {
                            "key": "OpenMainWindowShortcutKey",
                            "name": "打开主界面快捷键",
                            "type": "shortcut",
                            "default": "Alt+D"
                        }
                    ]
                },
                {
                    "key": "DownloadDiskCache",
                    "name": "下载磁盘缓存",
                    "options": [{
                            "key": "DownloadDiskCacheSettiing",
                            "name": "",
                            "type": "downloaddiskcachesetting",
                            "default": "128"
                        }
                    ]
                }
            ]
        }
    ]
}
